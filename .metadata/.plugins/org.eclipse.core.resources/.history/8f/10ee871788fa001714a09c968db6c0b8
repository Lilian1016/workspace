package edu.fju;

public class Bubble {

	public static void main(String[] args) {
		int[] numbers = {45,81,9,72,4,6};
		int temp;
		
		for(int i=0;i<=numbers.length;i++){
			if(numbers[i]<numbers[i+1]){
				temp=numbers[i];
				numbers[i]=numbers[i+1];
				numbers[i+1]=temp;
			
				
			}break;
		}
	
	for(int num:numbers){
       System.out.print(num+"\t");
}}
}