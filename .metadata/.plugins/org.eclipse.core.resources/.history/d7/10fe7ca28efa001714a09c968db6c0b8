package edu.fju;

import java.util.Scanner;

public class VendingMachine {
	
	public static void main(String[]args){
		Scanner scanner =new Scanner(System.in);
		int n=3;
	while(n!=0){
		int m=0;
		String money =scanner.nextLine();
		n = Integer.parseInt(money);
		switch(n){
		case 1:
			m=m+1;
			break;
		case 5:
			m=m+5;
			break;
		case 10:
			m=m+10;
			break;
		
		}System.out.print(m);
	}

	}}
