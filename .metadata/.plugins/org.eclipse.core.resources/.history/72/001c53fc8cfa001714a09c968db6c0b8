package edu.fju;

public class Bubble {

	public static void main(String[] args) {
		int[] numbers = {45,81,9,72,4,6};
		int temp;
		
		for(int i=0;i<numbers.length;i++){
			for(int j=1;j<numbers.length;j++){
			if(numbers[i]<numbers[j]){
				temp=numbers[i];
				numbers[i]=numbers[j];
				numbers[j]=temp;
			
				
			}
		}break;
		}
	for(int num:numbers){
       System.out.print(num+"\t");
}}
}